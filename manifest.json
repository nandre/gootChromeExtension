{
"name" :"Goot",
"version" :"0.1",
"description" :"Share your links !",
"manifest_version" : 2,
"background" : {
    "scripts": ["background.js"]
},
"content_scripts": [
	 {
	 "matches": ["*://*.goot.cfapps.io/facebookUser/syncFacebookUser?*"], //The page(s) where the content scripts will fire
	 "js": ["savetoken.js"],
	 "all_frames": true,
	 "run_at": "document_start"
	 }
 ],
"permissions": [
    "tabs",
	"https://graph.facebook.com/*",
	"https://facebook.com/connect/*"
  ],
"content_security_policy": "script-src 'self' https://connect.facebook.net; object-src 'self'",
"content_security_policy": "script-src 'self' https://graph.facebook.com; object-src 'self'",
"browser_action" :
    {
		"default_icon" : "icon.png",
        "default_popup": "popup.html"
    }
}